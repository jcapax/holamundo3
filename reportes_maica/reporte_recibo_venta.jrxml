	<parameter name="id_transaccion" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT t.id, t.id_lugar, l.descripcion as nombre_lugar, t.descripcion_transaccion, t.nro_tipo_transaccion,
t.fecha, d.descripcion as nombre_producto, d.simbolo, d.cantidad, 
d.valor_unitario, d.valor_total, v.fecha_vencimiento, d.descuento, t.usuario 
FROM `v_transaccion` t 
	join v_detalle_transaccion d on t.id = d.id_transaccion 
	join lugar l on t.id_lugar = l.id 
	left join vencimiento v on t.id = v.id_transaccion 
		and v.id_producto = d.id_producto 
		and v.id_unidad_medida = d.id_unidad_medida 
Where t.id = $P{id_transaccion}]]>
	</queryString>
