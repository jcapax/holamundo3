	<parameter name="id_lugar" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT *,  f_stock_producto(id_producto, id_unidad_medida, $P{id_lugar}) stock,
		f_nombre_lugar($P{id_lugar}) nombre_lugar                 
FROM v_productos 
WHERE estado = 'V' 
	and id_unidad_producto is not null
	and  f_stock_producto(id_producto, id_unidad_medida, $P{id_lugar}) > 0
ORDER BY 	f_stock_producto(id_producto, id_unidad_medida, $P{id_lugar}) asc;]]>
