	<parameter name="id_lugar" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT p.descripcion, p.nombre_unidad_medida, s.stock, l.descripcion nombre_lugar
FROM v_stock s
	JOIN lugar l ON s.id_lugar = l.id
	JOIN v_productos_lugar p ON p.id_producto = s.id_producto 
		AND p.id_unidad_medida = s.id_unidad_medida
WHERE l.id = $P{id_lugar}
ORDER BY s.stock]]>
	</queryString>
